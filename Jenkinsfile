    agent any
    stages {
        stage('stage 1') {
            steps {
               echo "stage 1"
            }
        }
        stage('install') {
            steps {
                echo "Software install to start the package"
            }
        }
        stage('test') {
            steps {
                echo "Test pipeline"
            }
        }
        stage('package') {
            steps {
                echo "Package added"
            }
        }
    }
}
