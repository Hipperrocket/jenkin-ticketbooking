            steps {
               echo "stage 1"
            }
        }
        stage('install') {
            steps {
                echo "First Test in Jenkins"
            }
        }
        stage('test') {
            steps {
                echo "Test pipeline"
            }
        }
        stage('package') {
            steps {
                echo "Second test in Package"
            }
        }
    }
}
